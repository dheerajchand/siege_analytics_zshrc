# Custom variables (export VAR="value")
# Example:
# export JARS_DIR="$HOME/.jars"
# export PYENV_DEFAULT_VENV="default_31111"

export JARS_DIR="${JARS_DIR:-$HOME/.jars}"
export ZSH_STACK_PROFILE="${ZSH_STACK_PROFILE:-stable}"
export PYENV_DEFAULT_VENV="${PYENV_DEFAULT_VENV:-default_31111}"
export SPARK_JARS_AUTO_DOWNLOAD="${SPARK_JARS_AUTO_DOWNLOAD:-1}"
export SPARK_SEDONA_ENABLE="${SPARK_SEDONA_ENABLE:-1}"
export SPARK_SEDONA_VERSION="${SPARK_SEDONA_VERSION:-1.8.1}"
export SPARK_GEOTOOLS_VERSION="${SPARK_GEOTOOLS_VERSION:-1.8.1-33.1}"
export SPARK_GRAPHFRAMES_ENABLE="${SPARK_GRAPHFRAMES_ENABLE:-1}"
export SPARK_GRAPHFRAMES_VERSION="${SPARK_GRAPHFRAMES_VERSION:-}"
export SPARK_LEGACY_JAR_DIR_ENABLE="${SPARK_LEGACY_JAR_DIR_ENABLE:-0}"
export SPARK_LOCAL_MASTER="${SPARK_LOCAL_MASTER:-local[*]}"
export SPARK_EXECUTION_MODE="${SPARK_EXECUTION_MODE:-auto}"
export SPARK_KAFKA_ENABLE="${SPARK_KAFKA_ENABLE:-1}"
export SPARK_KAFKA_VERSION="${SPARK_KAFKA_VERSION:-}"
export SPARK_VERSION="${SPARK_VERSION:-}"
export SPARK_SCALA_VERSION="${SPARK_SCALA_VERSION:-}"
export HADOOP_VERSION="${HADOOP_VERSION:-}"
export JAVA_VERSION="${JAVA_VERSION:-}"
export ZEPPELIN_PORT="${ZEPPELIN_PORT:-8081}"
export ZEPPELIN_SPARK_HOME="${ZEPPELIN_SPARK_HOME:-}"
export ZEPPELIN_SPARK_INTEGRATION_MODE="${ZEPPELIN_SPARK_INTEGRATION_MODE:-external}"
export ZEPPELIN_LIVY_URL="${ZEPPELIN_LIVY_URL:-http://127.0.0.1:8998}"
export ZEPPELIN_LIVY_STRICT="${ZEPPELIN_LIVY_STRICT:-0}"
export ZEPPELIN_USE_DAEMON="${ZEPPELIN_USE_DAEMON:-1}"
export LIVY_HOME="${LIVY_HOME:-$HOME/opt/livy/current}"
export LIVY_CONF_DIR="${LIVY_CONF_DIR:-$HOME/.config/livy}"
export LIVY_LOG_DIR="${LIVY_LOG_DIR:-$HOME/.config/livy/logs}"
export ZEPPELIN_CONF_DIR="${ZEPPELIN_CONF_DIR:-$HOME/.config/zeppelin}"
export ZEPPELIN_LOG_DIR="${ZEPPELIN_LOG_DIR:-$HOME/.config/zeppelin/logs}"
export ZEPPELIN_NOTEBOOK_DIR="${ZEPPELIN_NOTEBOOK_DIR:-$HOME/.config/zeppelin/notebook}"
